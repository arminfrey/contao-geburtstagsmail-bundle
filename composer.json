{
	"name":"arminfrey/contao-geburtstagsmail-bundle",
	"description":"Sends a birthday email to all the members having their birthday on the current day.",
	"keywords":["contao", "birthday", "birthdaymail", "members", "email"],
	"type":"contao-module",
	"license":"LGPL-3.0-or-later",
	"authors":[
		{
			"name":"Armin Frey"
		}
	],
	"support":{
		"issues":"https://github.com/arminfrey/contao-geburtstagsmail-bundle/issues",
		"source":"https://github.com/arminfrey/contao-geburtstagsmail-bundle"
	},
	"require":{
		"php":"^8.0",
		"contao/core-bundle":"^5.0",
		"doctrine/dbal": "^3.6"
	},
	"autoload": {
        	"psr-4": {
            		"Arminfrey\\GeburtstagsmailBundle\\": "src/"
       		 },
		"classmap": [
            		"src/contao"
        		]
    	},
	"extra":{
		"contao-manager-plugin": "Arminfrey\\GeburtstagsmailBundle\\ContaoManager\\Plugin"
	}
}
